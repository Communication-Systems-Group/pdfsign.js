{
  // Environments
  "browser": true,
  "devel": true,
  "worker": true,
  "predef": [
    "Promise",
    "require",
    "define",
    "exports"
  ],

  // Enforcing
  "maxlen": 80,
  "quotmark": "single",
  "trailing": true,
  "curly": true,
  "undef": true,
  "noarg": true,
  "nonbsp": true,
  "eqeqeq": true,

  // Relaxing
  "boss": true,
  "funcscope": true,
  "globalstrict": true,
  "loopfunc": true,
  "maxerr": 1000,
  "nonstandard": true,
  "sub": true,
  "validthis": true
}
